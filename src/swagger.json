            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Sessions"
              },
              "examples": {
                "Sessions": {
                  "value": {
                    "email": "vendedor.admin@teste.com",
                    "password": "123456"
                  }
                }
              }
            }
          }
        },
